{
  "name": "aotravel-backend-ultimate",
  "version": "2026.02.13",
  "description": "ðŸš€ AOtravel Server Ultimate - Sistema de Transporte Angola (Backend Completo) com Dashboard Visual em Tempo Real",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:watch": "nodemon --inspect server.js",
    "prod": "NODE_ENV=production node server.js",
    "db:init": "node -e \"require('./server.js').bootstrapDatabase()\"",
    "db:reset": "node scripts/reset-db.js",
    "db:migrate": "node scripts/migrate.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t aotravel-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env aotravel-backend",
    "docker:compose": "docker-compose up",
    "pm2:start": "pm2 start server.js --name aotravel-backend",
    "pm2:stop": "pm2 stop aotravel-backend",
    "pm2:restart": "pm2 restart aotravel-backend",
    "pm2:logs": "pm2 logs aotravel-backend",
    "pm2:monitor": "pm2 monit",
    "backup:db": "node scripts/backup-db.js",
    "health:check": "node scripts/health-check.js"
  },
  "keywords": [
    "aotravel",
    "transport",
    "angola",
    "ride-sharing",
    "taxi",
    "delivery",
    "real-time",
    "socket.io",
    "postgresql",
    "nodejs",
    "express",
    "dashboard",
    "monitoring",
    "cli-dashboard",
    "realtime-stats"
  ],
  "author": "AOtravel Team <dev@aotravel.ao>",
